4. create tabel orderitems
   insert data orderitems





create table customers(
    cust_id int primary key auto_increment,
    cust_name varchar(60),
    cust_address varchar(100),
    cust_city varchar(25),
    cust_state varchar(2),
    cust_zip varchar(5),
    cust_country varchar(3),		
    cust_contact varchar(60),
    cust_email varchar(100)
    );


insert into customers(cust_name,cust_address,cust_city,cust_state,cust_zip,cust_country,cust_contact,cust_email) VALUES('Mouse House','333 Fromage Lane','Columbus','OH','43333','USA','Jerry Mouse',''),

