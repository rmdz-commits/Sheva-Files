CREATE TABLE products(
 pro_id int primary key auto_increment,
 vend_id int,
 prod_name varchar(100),
 prod_price int,
 prod_desc varchar(100),
 constraint fk_vend_id foreign key(vend_id) references vendors(vend_id)
);


insert into products (vend_id,prod_name,prod_price,prod_desc) VALUES (1,'5 Ton Anvil',6,'5 ton anvil');

Insert Into products (vend id, prod_name,prod_price,prod_desc) VALUES (1,'1 Ton Anvil',10,'1 ton anvil');

insert into products (vend_id, prod_name,prod_price,prod_desc) VALUES (1,'1 Ton Anvil',15,'2 ton anvil');

insert into products (vend_id,prod_name,prod_price,prod_desc) VALUES (3,'Detonator',13,'Detenator');

